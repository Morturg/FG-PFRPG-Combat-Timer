	<template name="close_timer_settings">
		<close>
			<anchored>
				<top offset="0" />
				<right offset="-8" />
			</anchored>
		</close>
	</template>

	<template name="settings_field">
		<label>
			<anchored height="20" width="50">
				<top parent="contentanchor" anchor="bottom" relation="relative" offset="20" />
				<left offset="20" />
			</anchored>
			<frame name="headersimple" offset="10,3,10,3" />
		</label>
	</template>
</root>
